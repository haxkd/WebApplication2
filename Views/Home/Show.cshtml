@model System.Data.DataTable


<h1>All Datas:-- </h1>

<table border>
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Gender</th>
            <th>Show</th>
        </tr>
    </thead>
    <tbody>
        @foreach (System.Data.DataRow x in Model.Rows)
        {

            <tr>
                <td>@x["id"] </td>
                <td>@x["name"]</td>
                <td>@x["email"]</td>
                <td>@x["gender"]</td>
                <td><a href='/Home/Single/@x["id"]'>Show</a></td>
            </tr>
        }
    </tbody>
</table>